<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="description" content="{{ site.description }}">
    <meta name="author" content="{{ site.author }}">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>{{ site.title }}{% if page.title %} &mdash; {{ page.title }}{% endif %}</title>
    
    {% block css %}
        <!-- Bootstrap -->
        <link href="{{ pcurl('css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
        <link href="{{ pcurl('css/bootstrap-responsive.min.css') }}" rel="stylesheet" media="screen">
        <!-- FontAwesome -->
        <link href="{{ pcurl('css/font-awesome.min.css') }}" rel="stylesheet" media="screen">
        <!-- Google Code Prettify Theme -->
        <link href="{{ pcurl('css/prettify/desert.css') }}" type="text/css" rel="stylesheet" />
        <!-- Override Styles -->
        <link rel="stylesheet" type="text/css" href="{{ pcurl('css/styles.css') }}" />
    {% endblock %}

    {% block javascriptTop %}
        <!--[if lt IE 9]>
            <script src="{{site.root}}js/html5.js"></script>
        <![endif]-->

        <script src="http://code.jquery.com/jquery.js"></script>
        <script src="{{ pcurl('js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ pcurl('js/prettify/prettify.js') }}"></script>
        <script type="text/javascript">
            $(function() {
                $('pre').not('.prettyprint').filter('[class^=brush]').addClass('prettyprint linenums');
                prettyPrint();

                $('div.note').not('.alert').addClass('alert alert-info');
                $('div.new').not('.alert').addClass('alert alert-block');
                //$('.image-caption').not('.well').addClass('well');
            });
        </script>
    {% endblock %}
	
